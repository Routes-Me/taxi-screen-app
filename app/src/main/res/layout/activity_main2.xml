<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false"
    tools:context=".View.Activity.MainActivity"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/border"
        android:orientation="vertical">




        <!--homepage content-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">


            <!--75% OF SCREEN-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight=".25"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:background="@drawable/bg_color">


                    <ImageView
                        android:id="@+id/ADS_ImageView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/bg_color"/>

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/border">


                    <ImageView
                        android:id="@+id/ADS_VideoView_defaultImage"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitXY"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:background="@drawable/routeslabel"
                        android:visibility="visible"/>


                    <VideoView
                        android:id="@+id/ADS_VideoView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitXY"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:visibility="gone"/>

                    <com.google.android.exoplayer2.ui.SimpleExoPlayerView
                        android:id="@+id/ADS_exoPlayer_VideoView"
                        android:layout_width="1280px"
                        android:layout_height="720px"
                        android:scaleType="fitXY"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:visibility="invisible"
                        app:use_controller="true"
                       >
                    </com.google.android.exoplayer2.ui.SimpleExoPlayerView>






                </RelativeLayout>

            </LinearLayout>




            <!--25% OF SCREEN-->
            <FrameLayout
                android:id="@+id/frameLayout_home"
                android:layout_width="match_parent"
                android:layout_weight=".75"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:id="@+id/home_layout"
                    android:layout_weight=".75">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:background="@drawable/bg_color"
                        android:layout_height="80dp"
                        >


                        <ImageView
                            android:id="@+id/app_logo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_margin="10dp"
                            android:src="@drawable/tab_logo" />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/adminPatternView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/rsz_counter_background2"
                        android:tint="#6F000000"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:layout_weight="0.5"
                        android:visibility="gone">


                        <com.andrognito.patternlockview.PatternLockView
                            android:id="@+id/pattern_lock_view_admin"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/grad_bg"
                            app:normalStateColor="@color/textColor"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/adminSerialNoView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:tint="#6F000000"
                        android:orientation="horizontal"
                        android:layout_gravity="center"
                        android:layout_weight="0.5"
                        android:gravity="center"
                        android:padding="10dp"
                        android:visibility="visible">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Tablet Serial Number"
                            android:textColor="@color/colorPrimaryDark"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Large"
                            android:textSize="20dp"
                            android:padding="3dp"
                            android:visibility="gone"/>


                        <EditText
                            android:id="@+id/editText_addTabletSerialNo"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:hint="Tablet Serial No"
                            android:inputType="numberPassword"
                            android:singleLine="true"
                            android:layout_marginTop="8dp"
                            android:textColor="@color/colorPrimaryDark"/>


                        <TextView
                            android:id="@+id/button_addTabletSerialNo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/save"
                            android:textAppearance="@style/TextAppearance.AppCompat.Large"
                            android:textSize="20dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textStyle="bold"
                            android:background="@drawable/buttonstyle"
                            android:layout_marginTop="16dp"
                            android:padding="5dp"/>



                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/userView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/rsz_counter_background2"
                        android:tint="#6F000000"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:layout_weight="0.5"
                        android:visibility="gone">


                        <ImageView
                            android:id="@+id/btn_selectLang"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:src="@drawable/english_flag"
                            android:layout_gravity="right"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="3dp"
                            android:layout_marginBottom="3dp"
                            android:visibility="visible"/>


                        <LinearLayout
                            android:id="@+id/languageLayout"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:visibility="gone"
                            android:padding="5dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_margin="5dp">




                                    <TextView
                                        android:id="@+id/btn_englishLang"
                                        android:layout_weight="1"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:background="@drawable/round_button"
                                        android:padding="6dp"
                                        android:layout_marginRight="5dp"
                                        android:textColor="#245283"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                        android:text="English"
                                        android:gravity="center"
                                        android:textSize="18dp"
                                        android:textStyle="bold"
                                        />



                                <TextView
                                    android:id="@+id/btn_arabicLang"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="@drawable/round_button"
                                    android:padding="6dp"
                                    android:layout_marginLeft="5dp"
                                    android:textColor="#245283"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:text="Arabic"
                                    android:gravity="center"
                                    android:textSize="18dp"
                                    android:textStyle="bold"
                                    />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_margin="5dp">

                                <TextView
                                    android:id="@+id/btn_tagalogLang"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="@drawable/round_button"
                                    android:padding="6dp"
                                    android:layout_marginRight="5dp"
                                    android:textColor="#245283"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:text="Tagalog"
                                    android:gravity="center"
                                    android:textSize="18dp"
                                    android:textStyle="bold"
                                    />

                                <TextView
                                    android:id="@+id/btn_urduLang"
                                    android:layout_weight="1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:background="@drawable/round_button"
                                    android:padding="6dp"
                                    android:layout_marginLeft="5dp"
                                    android:textColor="#245283"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                    android:text="Urdu"
                                    android:gravity="center"
                                    android:textSize="18dp"
                                    android:textStyle="bold"
                                    />

                            </LinearLayout>

                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/counterLayout"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:visibility="visible"
                           >


                        <TextView
                            android:id="@+id/DayClock"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="THU"
                            android:textColor="#245283"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Title"
                            android:textSize="30dp"/>
                        <TextView
                            android:id="@+id/timeClock"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="03:51:21 PM"
                            android:textColor="#245283"
                            android:gravity="center"
                            android:layout_marginTop="2dp"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="30dp"/>

                        <TextView
                            android:id="@+id/DateClock"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="12 June. 2019"
                            android:textColor="#245283"
                            android:gravity="center"
                            android:layout_marginTop="2dp"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="30dp"/>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_marginTop="2dp"
                            android:visibility="gone">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Duration"
                                android:textColor="#245283"
                                android:textStyle="bold"
                                android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                                android:layout_marginRight="2dp"
                                android:textSize="25dp"/>

                            <Chronometer
                                android:id="@+id/chronometer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#245283"
                                android:textStyle="bold"
                                android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                                android:layout_marginRight="3dp"
                                android:textSize="25dp"
                                />



                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=" ,Dist."
                                android:textColor="#245283"
                                android:textStyle="bold"
                                android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                                android:layout_marginRight="2dp"
                                android:textSize="25dp"/>


                            <TextView
                                android:id="@+id/DistanceView"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0 KM"
                                android:textColor="#245283"
                                android:textStyle="bold"
                                android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                                android:textSize="25dp"/>

                        </LinearLayout>




                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:layout_marginTop="2dp"
                            android:visibility="gone">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Price"
                                android:textStyle="bold"
                                android:textColor="#245283"
                                android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                                android:textSize="25dp"
                                android:layout_marginRight="2dp"
                                />


                            <TextView
                                android:id="@+id/PriceView"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.0 KD"
                                android:textStyle="bold"
                                android:textColor="#245283"
                                android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                                android:textSize="25dp"
                                />

                        </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:background="@color/white"
                        android:layout_weight=".3">




