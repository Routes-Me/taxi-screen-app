                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                    android:textColor="@color/textColor"
                                    android:textSize="16dp"
                                    android:textStyle="bold"
                                    android:layout_gravity="right"
                                    android:layout_alignParentRight="true"
                                    android:layout_margin="3dp"
                                    />



                                <LinearLayout

                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:gravity="center"
                                    android:padding="3dp">






                                    <TextView
                                        android:id="@+id/tvEnterPhoneNumber"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="18dp"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                        android:text="@string/please_enter_your_phone_number"
                                        android:textColor="#fff"
                                        android:textStyle="bold"
                                        />





                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        android:gravity="center"
                                        android:layout_marginBottom="7dp">


                                        <com.hbb20.CountryCodePicker
                                            android:id="@+id/ccp"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            app:ccp_defaultNameCode="KW"
                                            app:ccpDialog_backgroundColor="@color/colorPrimaryDark"
                                            app:ccpDialog_textColor="@color/textColor"
                                            app:ccp_contentColor="@color/textColor"
                                            />

                                        <EditText
                                            android:id="@+id/editText_phoneNumber_AddItemLayoutID"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:hint="phone"
                                            android:inputType="numberPassword"
                                            android:singleLine="true" />





                                    </LinearLayout>


                                    <Button
                                        android:id="@+id/btnSave_AddItemLayoutID"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/save"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                        android:textColor="@color/textColor"
                                        android:textSize="18dp"
                                        android:textStyle="bold"
                                        android:background="@drawable/buttonstyle_white"
                                        android:layout_marginLeft="30dp"
                                        android:layout_marginRight="30dp"
                                        android:layout_gravity="right"
                                        />







                                    <!--
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        android:gravity="center"
                                        android:layout_marginBottom="16dp">



                                        <ImageView
                                            android:layout_width="30dp"
                                            android:layout_height="30dp"
                                            android:src="@drawable/kuwait_flag"
                                            android:layout_marginRight="4dp"/>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="+965"
                                            android:textColor="#fff"
                                            android:textSize="16dp"
                                            android:textStyle="bold"
                                            android:layout_marginRight="6dp"/>

                                        <EditText
                                            android:id="@+id/editText_phoneNumber_AddItemLayoutID"
                                            android:layout_width="200dp"
                                            android:layout_height="wrap_content"
                                            android:inputType="number"
                                            android:textColor="@color/textColor"/>




                                    </LinearLayout>
                -->







                                    <TextView
                                        android:id="@+id/tvScanQRCode"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:layout_marginBottom="10dp"
                                        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                        android:text="@string/or_scan_qr_code"
                                        android:textColor="#fff"
                                        android:textStyle="bold"
                                        android:layout_marginTop="25dp"
                                        />

                                    <ImageView
                                        android:layout_width="100dp"
                                        android:layout_height="100dp"
                                        android:background="@drawable/qrcode"
                                        />







                                </LinearLayout>

                            </RelativeLayout>







                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>

            </FrameLayout>

        </LinearLayout>




        <!--news scroll-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:id="@+id/scroll_news"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:background="@drawable/bg_color"
                android:layout_gravity="center"
                android:gravity="center"
